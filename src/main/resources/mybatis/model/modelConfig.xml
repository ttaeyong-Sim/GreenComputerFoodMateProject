<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
	PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
	"../mybatis-3-config.dtd">
<configuration>
	<!-- ▼<typeAliases> 위에 입력해야된다 -->
	<settings>
        <setting name="jdbcTypeForNull" value="NULL" />
    </settings>

	<typeAliases>
		<typeAlias type="com.spring.FoodMate.member.dto.BuyerDTO" alias="buyerDTO"/>
		<typeAlias type="com.spring.FoodMate.member.dto.SellerDTO" alias="sellerDTO"/>
		<typeAlias type="com.spring.FoodMate.member.dto.AdminDTO" alias="adminDTO"/>
		<typeAlias type="com.spring.FoodMate.mypage.dto.ProfileDTO" alias="profileDTO"/>
		<typeAlias type="com.spring.FoodMate.product.dto.ProductDTO" alias="productDTO"/>
		<typeAlias type="com.spring.FoodMate.product.dto.CategoryDTO" alias="categoryDTO"/>
		<typeAlias type="com.spring.FoodMate.cart.dto.CartDTO" alias="cartDTO"/>
		<typeAlias type="com.spring.FoodMate.order.dto.OrderDTO" alias="orderDTO"/>
		<typeAlias type="com.spring.FoodMate.recipe.dto.RecipeDTO" alias="recipeDTO"/>
        <typeAlias type="com.spring.FoodMate.recipe.dto.RecipeIngredientDTO" alias="recipeIngredientDTO"/>
        <typeAlias type="com.spring.FoodMate.recipe.dto.RecipeStepDTO" alias="recipeStepDTO"/>
	</typeAliases>
	
</configuration>